<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="CustonActionBarTheme" parent="@android:style/Theme.Holo.Light.DarkActionBar">
        <item name="android:actionBarStyle">@style/MyActionBar</item>
    </style>

    <style name="MyActionBar" parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse">
        <item name="android:background">#ff8800</item>
    </style>




    <style name="CustonActionBarTheme2" parent="@android:style/Theme.Holo.Light.DarkActionBar">
        <item name="android:actionBarStyle">@style/MyActionBar2</item>
        <item name="android:actionBarTabTextStyle">@style/MyActionBarTitleText</item>
    </style>


    <style name="MyActionBar2" parent="@android:style/Widget.Holo.Light.ActionBar">
        <item name="android:titleTextStyle">@style/MyActionBarTitleText</item>
    </style>


    <style name="MyActionBarTitleText" parent="@android:style/TextAppearance.Holo.Widget.ActionBar.Title">
        <item name="android:textColor">#ffffff</item>
    </style>







</resources>